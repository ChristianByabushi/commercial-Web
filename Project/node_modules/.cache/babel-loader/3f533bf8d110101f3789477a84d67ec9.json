{"ast":null,"code":"import alert from '@/components/alert.vue';\nimport axios from 'axios';\nexport default {\n  name: \"Dashboard\",\n  components: {\n    alert\n  },\n  data: () => ({\n    specificResult: [],\n    generalResult: [],\n    DetailsItems: []\n  }),\n  methods: {\n    async getDashboardanalysis() {\n      try {\n        const formData = new FormData();\n        const response = await axios.post('lineInvoice/dashboardanalysis');\n        var result = response.data;\n        this.specificResult = [{\n          title: \"Total de produits achétés\",\n          color: \"#FD00FFFF\",\n          amounts: result.totalAmountby.totalAmountBought + ' Units'\n        }, {\n          title: 'Total de produits Vendus',\n          color: '#FF8800FF',\n          amounts: result.totalAmountSold.totalAmountSold + ' Units'\n        }, {\n          title: 'Le plus grand résulat réalisé',\n          color: '#00B8FFFF',\n          amounts: result.highSoldAchieved.result + ' $ '\n        }, {\n          title: 'Différence entre les achats et les ventes',\n          color: '#00FF69DE',\n          amounts: result.getResultInvoices[1].result + ' $ '\n        }];\n        this.DetailsItems = result.DetailsItems;\n      } catch (e) {\n        console.log(e);\n      }\n    },\n\n    getDate() {// var currentDate = new Date();\n      // var mydashdate = new Date(currentDate.setDate(currentDate.getDate() - 1));\n      // console.log(mydashdate.getMonth() + 1)\n    }\n\n  },\n\n  created() {\n    this.getDashboardanalysis();\n    this.getDate();\n  }\n\n};","map":{"version":3,"mappings":"AAsDA;AACA;AAEA;EACAA,iBADA;EAEAC;IACAC;EADA,CAFA;EAKAC;IACAC,kBADA;IAEAC,iBAFA;IAGAC;EAHA,EALA;EAWAC;IACA;MACA;QACA;QACA;QACA;QACA,uBACA;UAAAC;UAAAC;UAAAC;QAAA,CADA,EAEA;UAAAF;UAAAC;UAAAC;QAAA,CAFA,EAGA;UAAAF;UAAAC;UAAAC;QAAA,CAHA,EAIA;UAAAF;UAAAC;UAAAC;QAAA,CAJA;QAMA;MACA,CAXA,CAWA;QACAC;MACA;IACA,CAhBA;;IAiBAC,WACA;MACA;MACA;IAEA;;EAtBA,CAXA;;EAoCAC;IACA;IACA;EACA;;AAvCA","names":["name","components","alert","data","specificResult","generalResult","DetailsItems","methods","title","color","amounts","console","getDate","created"],"sourceRoot":"src/views/Dashboard","sources":["index.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard\">\r\n    <v-subheader class=\"d-flex justify-space-between align-center\">\r\n      <h3>Dashboard</h3>\r\n      <v-btn color=\"#ffff\" link to=\"/merchandise\">\r\n        Voir Details\r\n      </v-btn>\r\n    </v-subheader>\r\n\r\n    <v-row>\r\n      <v-col lg=\"6\" cols=\"12\">\r\n        <v-alert dense type=\"success\" color=\"#0091EA\">\r\n          Connexion réussie avec succès <strong>Bon travail</strong> !\r\n        </v-alert>\r\n        <v-row>\r\n          <v-col lg=\"4\" cols=\"12\" v-for=\"(item, index) in specificResult\" :key=\"index\">\r\n            <v-card elevation=\"4\" class=\"rounded-lg\">\r\n              <v-card-text class=\"d-flex justify-space-between align-center\">\r\n                <div class=\"inline\">\r\n                  <strong>{{item.title}}</strong>\r\n                  <small> (2 dernières semaines)</small>\r\n                </div>\r\n                <v-avatar :color=\"item.color\" size=\"60\">\r\n                  <span class=\"\">{{item.amounts}}</span>\r\n                </v-avatar>\r\n              </v-card-text>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </v-col>\r\n      <v-col lg=\"6\" cols=\"12\">\r\n        <v-card>\r\n          <v-card-title>Biens auquels sont liés les activités</v-card-title>\r\n          <v-timeline dense>\r\n            <v-timeline-item v-for=\"item in DetailsItems\" :key=\"item.id\">\r\n              <span slot=\"opposite\">item.title</span>\r\n              <v-card class=\"elevation-2\">\r\n                <v-card-title class=\"text-h5\">\r\n                  {{item.categorie}}\r\n                </v-card-title>\r\n                <v-card-text>\r\n                  {{item.description}}\r\n                </v-card-text>\r\n              </v-card>\r\n            </v-timeline-item>\r\n          </v-timeline>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport alert from '@/components/alert.vue'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: \"Dashboard\",\r\n  components: {\r\n    alert\r\n  },\r\n  data: () => ({\r\n    specificResult: [],\r\n    generalResult: [],\r\n    DetailsItems: []\r\n\r\n  }),\r\n  methods: {\r\n    async getDashboardanalysis() {\r\n      try {\r\n        const formData = new FormData()\r\n        const response = await axios.post('lineInvoice/dashboardanalysis',)\r\n        var result = response.data\r\n        this.specificResult = [\r\n          { title: \"Total de produits achétés\", color: \"#FD00FFFF\", amounts: result.totalAmountby.totalAmountBought + ' Units' },\r\n          { title: 'Total de produits Vendus', color: '#FF8800FF', amounts: result.totalAmountSold.totalAmountSold + ' Units' },\r\n          { title: 'Le plus grand résulat réalisé', color: '#00B8FFFF', amounts: result.highSoldAchieved.result + ' $ ' },\r\n          { title: 'Différence entre les achats et les ventes', color: '#00FF69DE', amounts: result.getResultInvoices[1].result + ' $ ' },\r\n        ]\r\n        this.DetailsItems = result.DetailsItems\r\n      } catch (e) {\r\n        console.log(e)\r\n      }\r\n    },\r\n    getDate() {\r\n      // var currentDate = new Date();\r\n      // var mydashdate = new Date(currentDate.setDate(currentDate.getDate() - 1));\r\n      // console.log(mydashdate.getMonth() + 1)\r\n\r\n    }\r\n\r\n  },\r\n  created() {\r\n    this.getDashboardanalysis()\r\n    this.getDate()\r\n  }\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}